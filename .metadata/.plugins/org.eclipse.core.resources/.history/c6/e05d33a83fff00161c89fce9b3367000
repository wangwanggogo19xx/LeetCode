package com.zw.leetCode_70;

public class Solution {
    public int climbStairs(int n) {
    	return climb(n,0);
    }
    public int climb(int n,int result){
    	if(n==0) return result+1;
    	if(n==-1) return result;
        for(int i=0;i<=1;i++){
        	if(i==0){ 
        		climb(n-1,result);
        	}
        	else{
        		climb(n-2,result);
        	}
        }
        return result;
    }
}