	String sb = "a.";
	sb = sb.replaceAll("[^a-zA-Z]", "");
	System.out.println(isPalindrome(sb));

	}
	 public static boolean isPalindrome(String s) {
	        s = s.replace("[^a-zA-Z]","");
	        int i=0,j=s.length()-1;
	        while(j>=i){
	            if(s.charAt(i) != s.charAt(j)) return false;
	            i++;
	            j--;
	        }
	        return true;
	    }

}